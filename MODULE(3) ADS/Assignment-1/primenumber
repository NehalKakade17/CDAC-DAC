class primenum {
    public static boolean show(int num1,int num2 ){
        if(num1%num2==0){
            return false;
        }
        if(num2*num2>num1){
            return true;
        }
        return show(num1,num2+1);
    }
    public static void main(String[] args) {
        System.out.println(show(14,2));
    }
}
